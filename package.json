{
    "name": "spliscord",
    "version": "0.2.0",
    "description": "Spliscord is a Discord bot made using the Discord.js wrapper for Discord's API. Spliscord is actively developed with new features being added and tested almost every week.",
    "main": "dist/lib/main.js",
    "engines": {
        "node": "8.9.0"
    },
    "scripts": {
        "test": "mocha -r ts-node/register src/**/*.spec.ts",
        "build": "tsc",
        "start": "pm2 start src/main.ts -n \"spliscord\" ",
        "restart": "pm2 restart spliscord",
        "stop": "pm2 stop spliscord"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jdenderplays/Spliscord.git"
    },
    "contributors": [
        {
            "name": "jdenderplays",
            "email": "jdenderplays@gmail.com",
            "url": "https://github.com/jdenderplays"
        }
    ],
    "license": "AGPL-3.0",
    "bugs": {
        "url": "https://github.com/jdenderplays/Spliscord/issues"
    },
    "homepage": "https://github.com/jdenderplays/Spliscord#readme",
    "dependencies": {
        "discord.js": "^11.3.0",
        "lodash": "^4.17.5",
        "minimist": "^1.2.0",
        "node-opus": "^0.2.7",
        "phin": "^2.9.0",
        "redux": "^3.7.2",
        "redux-state-save": "^1.0.0",
        "reflect-metadata": "^0.1.12"
    },
    "optionalDependencies": {
        "erlpack": "^0.1.2",
        "libsodium-wrappers": "^0.5.4",
        "uws": "^9.14.0",
        "zlib-sync": "^0.1.4"
    },
    "devDependencies": {
        "@types/chai": "^4.1.2",
        "@types/lodash": "^4.14.101",
        "@types/minimist": "^1.2.0",
        "@types/mocha": "^2.2.48",
        "@types/node": "^9.4.0",
        "chai": "^4.1.2",
        "mocha": "^5.0.1",
        "pm2": "^2.9.3",
        "ts-node": "^4.1.0",
        "typescript": "^2.7.1"
    }
}
