{
  "name": "spliscord",
  "version": "0.2.0",
  "description": "Spliscord is a Discord bot made using the Discord.js wrapper for Discord's API. Spliscord is actively developed with new features being added and tested almost every week.",
  "main": "src/main.ts",
  "engines": {
    "node": "8.9.0"
  },
  "scripts": {
    "test": "ts-node -r dotenv/config .",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "start": "npm run build && ts-node .",
    "build": "parcel build public/index.html",
    "parcel": "parcel public/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdenderplays/Spliscord.git"
  },
  "contributors": [
    "jdender~ <jdenderplays@gmail.com> (https://github.com/jdenderplays)"
  ],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/jdenderplays/Spliscord/issues"
  },
  "homepage": "https://github.com/jdenderplays/Spliscord#readme",
  "dependencies": {
    "chalk": "^2.4.1",
    "discord.js": "^11.3.0",
    "express": "^4.16.2",
    "font-awesome": "^4.7.0",
    "klaw": "^2.1.1",
    "minimist": "^1.2.0",
    "moment": "^2.22.0",
    "node-opus": "^0.2.7",
    "rxjs": "^5.5.10",
    "sqlite3": "^4.0.0",
    "tinycolor2": "^1.4.1",
    "typeorm": "^0.1.13"
  },
  "optionalDependencies": {
    "erlpack": "^0.1.2",
    "libsodium-wrappers": "^0.7.3",
    "uws": "^9.14.0",
    "zlib-sync": "^0.1.4"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/klaw": "^2.1.1",
    "@types/minimist": "^1.2.0",
    "@types/node": "^9.4.6",
    "@types/tinycolor2": "^1.4.0",
    "dotenv": "^5.0.1",
    "node-sass": "^4.8.3",
    "parcel-bundler": "^1.7.1",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  }
